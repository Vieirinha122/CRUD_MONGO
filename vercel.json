
{
    "version": 2,
    "builds": [
      {
        "src": "src/App.ts",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/App.ts"
      },
      {
        "src": "/(.+)",
        "dest": "/public/$1"
      }
    ]
  }
  